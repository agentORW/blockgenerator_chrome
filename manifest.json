{
    "name": "Color picker",
    "description": "Im stronger, im smarter, im better",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "./Popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./contentScript.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["data/1.12.2 block textures/*"]
    }]
}